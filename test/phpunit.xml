<phpunit bootstrap="Bootstrap.php">
    <testsuite name="MyModuleTest">
        <directory>MtZendeskApiTest</directory>
    </testsuite>
    <filter>
        <whitelist addUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">../src</directory>
        </whitelist>
        <blacklist>
            <directory>../vendor</directory>
        </blacklist>
    </filter>
</phpunit>
